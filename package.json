{
  "name": "@tsavo/creatify-api-ts",
  "version": "1.0.0",
  "description": "TypeScript client for the Creatify AI API",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build",
    "prepare": "npm run build",
    "lint": "tsc --noEmit"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "keywords": [
    "creatify",
    "api",
    "typescript",
    "ai",
    "avatar",
    "video",
    "text-to-speech",
    "url-to-video",
    "ai-editing",
    "custom-templates",
    "dyoa"
  ],
  "homepage": "https://github.com/TSavo/creatify-api-ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TSavo/creatify-api-ts.git"
  },
  "bugs": {
    "url": "https://github.com/TSavo/creatify-api-ts/issues"
  },
  "author": {
    "name": "TSavo",
    "email": "creatify-api-ts@nefariousplan.com",
    "url": "https://github.com/TSavo"
  },
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.2",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "axios": "^1.6.2"
  }
}